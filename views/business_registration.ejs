  <script>
    $(function() {
      //請求書宛名選択時の切り替え
      $('[name="claim_name"]').change(function() {
        var val = $('[name="claim_name"]:checked').val();
        var $target = $('#claim_detail');
        if(val == '固定') {
          $target.fadeIn();
        } else {
          $target.find('input').prop('checked', false);
          claimErrorHide();
          $target.fadeOut();
        }
      });

      //フォーム送信時のバリデーション
      $('#submit').click(function() {
        if($('[name="claim_name"]:checked').val() == '固定' && $('[name="claim_detail"]:checked').length == 0) {
          claimErrorShow();
        } else {
          claimErrorHide();
        }
      });
      $('[name="claim_detail"]').change(function() {
        if($('[name="claim_detail"]:checked').length == 0) {
          claimErrorShow();
        } else {
          claimErrorHide();
        }
      });
    });
    function claimErrorShow() {
      $('#claim_detail').addClass('is-error');
      $('#claim_error').show();
    }
    function claimErrorHide() {
      $('#claim_detail').removeClass('is-error');
      $('#claim_error').hide();
    }
  </script>



